local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Rxr Shop", HidePremium = false})
_G.auto = true
_G.autoC = true
function auto()
	while _G.auto == true do
		local args = {
			[1] = "Input",
			[2] = "Undead FistsDoombringer's Assault",
			[3] = 0,
			[4] = "Fisticuffs",
			[5] = 3,
			[6] = true
		}
		
		game:GetService("Players").LocalPlayer.Character.Combat.RemoteEvent:FireServer(unpack(args))
		
		wait(.0001)
	end
     end
function autoC()
		while _G.auto == true do
          local args = {
				[1] = "Input",
				[2] = "Undead FistsDoombringer's Assault",
				[3] = 0,
				[4] = "Curse"
			}
			
			game:GetService("Players").LocalPlayer.Character.Combat.RemoteEvent:FireServer(unpack(args))
			wait(.0001)
		end
		end

local Tab = Window:MakeTab({
	Name = "farm",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "auto PRIVATE V "
})

Tab:AddButton({
	Name = "auto Collect material",
	Callback = function()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")

for i = 1, math.huge do
    local args = {
        [1] = "Collect",
        [2] = tostring(i)
    }

    ReplicatedStorage:WaitForChild("RemoteEvent"):FireServer(unpack(args))

    wait(0.1)
end
  	end    
})

Tab:AddButton({
	Name = "spawn grizz lv2",
	Callback = function()
      		local args = {
    [1] = "Equip",
    [2] = "3393213709:52004"
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
  	end    
})


