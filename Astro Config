local Section = Tab:AddSection({
	Name = "auto doombringer assualt"
})

Tab:AddToggle({
	Name = "spam doombringerassualt",
	Default = false,
	Callback = function(Value)
		_G.auto = Value
		auto()
	end    
})



Tab:AddToggle({
	Name = "spam curse",
	Default = false,
	Callback = function(Value)
		_G.autoC = Value
		autoC()
	end    
})



